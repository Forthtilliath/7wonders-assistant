{
  "name": "7wonders-assistant",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "forth:merge": "git checkout dev && git merge dev-forth && git push origin dev:dev && git checkout dev-forth",
    "mike:merge": "git checkout dev && git merge dev-mike && git push origin dev:dev && git checkout dev-mike",
    "forth:pull": "git pull origin dev:dev-forth",
    "mike:pull": "git pull origin dev:dev-mike"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0"
  },
  "devDependencies": {
    "@capacitor/android": "^5.4.1",
    "@capacitor/camera": "^5.0.7",
    "@capacitor/cli": "^5.4.1",
    "@capacitor/core": "^5.4.1",
    "@capacitor/filesystem": "^5.1.4",
    "@capacitor/preferences": "^5.0.6",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.16",
    "clsx": "^2.0.0",
    "cypress": "^13.3.0",
    "eslint": "^8.50.0",
    "eslint-config-next": "^13.5.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "framer-motion": "^10.16.4",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "query-string": "^8.1.0",
    "react-image-crop": "^10.1.8",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.5"
  }
}
