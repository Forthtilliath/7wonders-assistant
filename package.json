{
  "name": "7wonders-assistant",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "forth:merge": "tsc && git checkout dev && git merge dev-forth && git push origin dev:dev && git checkout dev-forth",
    "mike:merge": "git checkout dev && git merge dev-mike && git push origin dev:dev && git checkout dev-mike",
    "pull": "git pull origin dev",
    "test": "vitest --ui",
    "tsc": "tsc --watch",
    "android:add": "npx cap add android",
    "android:copy": "npx cap copy android",
    "android:sync": "npx cap sync android",
    "android:open": "npx cap open android",
    "android:build": "pnpm build && pnpm android:add && pnpm android:copy && pnpm android:open",
    "android:update": "pnpm build && pnpm android:sync && pnpm android:open"
  },
  "dependencies": {
    "i18next": "^23.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.3.1",
    "react-router-dom": "^6.17.0"
  },
  "devDependencies": {
    "@capacitor/android": "^5.5.0",
    "@capacitor/camera": "^5.0.7",
    "@capacitor/cli": "^5.5.0",
    "@capacitor/core": "^5.5.0",
    "@capacitor/filesystem": "^5.1.4",
    "@capacitor/preferences": "^5.0.6",
    "@tailwindcss/container-queries": "^0.1.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@trivago/prettier-plugin-sort-imports": "^4.2.1",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.7",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/ui": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "chart.js": "^4.4.0",
    "clsx": "^2.0.0",
    "eslint": "^8.52.0",
    "eslint-config-next": "^13.5.6",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "framer-motion": "^10.16.4",
    "happy-dom": "^12.9.1",
    "html2canvas": "^1.4.1",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.2.2",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "query-string": "^8.1.0",
    "react-chartjs-2": "^5.2.0",
    "react-image-crop": "^10.1.8",
    "react-json-view": "^1.21.3",
    "start-server-and-test": "^2.0.1",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6",
    "zod": "^3.22.4",
    "zustand": "^4.4.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "react-json-view>react": "18",
        "react-json-view>react-dom": "18",
        "flux>react": "18"
      }
    }
  }
}